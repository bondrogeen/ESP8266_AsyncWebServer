    "gulp-gzip": "^1.4.2",
    "gulp-htmlmin": "5.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-rigger": "^0.5.8",
    "gulp-uglify": "^3.0.1",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.1.1",
    "node-sass": "^5.0.0"
  },
