{
  "name": "dot-nodemcu",
  "version": "0.2.1",
  "description": "Webserver for NodeMCU lua.",
  "main": "gulpfile.js",
  "directories": {
    "doc": "doc",
    "example": "example"
  },
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "css-build": "node-sass --omit-source-map-url sass/style.scss src/assets/css/style.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch"
  },
  "repository": {
